    "release:publish:ci": "./scripts/release/publish.js",
    "release:version": "./scripts/release/version.js"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "execa": "^5.1.1",
    "is-ci": "^3.0.0",
    "jest": "^27.1.0",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.3.5",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
